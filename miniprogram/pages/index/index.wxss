page{
  margin-top: 120rpx;
}
.workStudyProgram {
  width: 100%;
  height: 100%;
  display: flex;
  float: left;
}
.workStudyProgram .workStudyLeft {
  flex: 1;
  margin: 20rpx;
  margin-top: 60rpx;
}
.workStudyProgram .workStudyLeft image {
  border-radius: 10rpx;
  width: 200rpx;
  height: 200rpx;
}
.workStudyProgram .workStudyRight {
  flex: 2;
}
.workStudyProgram .workStudyRight .workStudyText {
  color: black;
  font-size: 24rpx;
}
.workStudyProgram .workStudyRight .workStudyButton {
  font-size: 25rpx;
  color: white;
  width: 120rpx;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  border-radius: 20rpx;
  background-color: #ff5507;
}
.swiper {
  width: 100%;
  height: 100%;
}
.swiper-item image{
  border-radius: 10rpx;
}
/* 默认指示点的样式 */
.swiper .wx-swiper-dot {
  width: 35rpx;
  height: 10rpx;
  background: white;
  border-radius: 15rpx;
}
/* 选中指示点的样式 */
.swiper .wx-swiper-dot.wx-swiper-dot-active {
  width: 35rpx;
  height: 10rpx;
  background: white;
  border-radius: 15rpx;
}
video{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
/* 时间轴组件 */
.timeline{
  margin-top: 80rpx;
}